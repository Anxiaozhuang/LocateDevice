<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:id="@+id/ScrollView01"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:fillViewport="true" android:fadeScrollbars="true">
	<LinearLayout android:orientation="vertical"
		android:layout_width="fill_parent" android:layout_height="fill_parent">
		<TextView android:layout_width="fill_parent"
			android:layout_height="wrap_content" android:textSize="11pt"
			android:textColor="#0f0" android:text="Signal Strength Table" />
		<TableLayout android:id="@+id/TableLayout_SSList"
			android:layout_width="wrap_content" android:layout_height="wrap_content"
			android:stretchColumns="*">
			<TableRow android:id="@+id/TableRow01" android:layout_width="wrap_content"
				android:layout_height="wrap_content">
				<TextView android:id="@+id/txtSS1" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="#f00"
					android:text="SS1"></TextView>
				<TextView android:id="@+id/txtSS2" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="#f00"
					android:text="SS2"></TextView>
				<TextView android:id="@+id/txtSS3" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="#f00"
					android:text="SS3"></TextView>
				<TextView android:id="@+id/txtClass" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="#f00"
					android:text="Classification"></TextView>
				<TextView android:id="@+id/txtDate" android:layout_width="wrap_content"
					android:layout_height="wrap_content" android:textColor="#f00"
					android:text="Added Date"></TextView>
			</TableRow>
		</TableLayout>
	</LinearLayout>
</ScrollView>